{
    "version": 2,
    "builds": [{"src": "index.js", "use": "@vercel/node"}],
   "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    },
     {
      "src": "/payments/create",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/payment",
      "dest": "/index.js",
      "methods": ["GET"]
    }
   ]
}